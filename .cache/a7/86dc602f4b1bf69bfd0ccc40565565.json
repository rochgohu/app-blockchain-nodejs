"line":49,"column":59}},{"generated":{"line":130,"column":0},"source":"components/Blocks.js","original":{"line":49,"column":28}},{"generated":{"line":130,"column":10},"source":"components/Blocks.js","original":{"line":48,"column":24}},{"generated":{"line":131,"column":0},"source":"components/Blocks.js","original":{"line":51,"column":21}},{"generated":{"line":131,"column":7},"source":"components/Blocks.js","original":{"line":47,"column":20}},{"generated":{"line":131,"column":8},"source":"components/Blocks.js","original":{"line":28,"column":12}},{"generated":{"line":131,"column":9},"source":"components/Blocks.js","original":{"line":27,"column":8}},{"generated":{"line":132,"column":0},"source":"components/Blocks.js","original":{"line":55,"column":5}},{"name":"Component","generated":{"line":136,"column":2},"source":"components/Blocks.js","original":{"line":6,"column":21}},{"name":"Blocks","generated":{"line":138,"column":15},"source":"components/Blocks.js","original":{"line":58,"column":15}}],"sources":{"components/Blocks.js":"import React, { Component } from 'react';\nimport { Link } from 'react-router-dom';\nimport { Button } from 'react-bootstrap';\nimport Block from './Block';\n\nclass Blocks extends Component {\n    state = { blocks: [], paginatedId: 1, blocksLength: 0 };\n\n    componentDidMount() {\n        fetch(`${document.location.origin}/api/blocks/length`)\n            .then(response => response.json())\n            .then(json => this.setState({ blocksLength: json }));\n\n        this.fetchPaginatedBlocks(this.state.paginatedId)();\n    }\n\n    fetchPaginatedBlocks = paginatedId => () => {\n        fetch(`${document.location.origin}/api/blocks/${paginatedId}`)\n            .then(response => response.json())\n            .then(json => this.setState({ blocks: json }));\n    }\n\n    render() {\n\n        console.log('this.state', this.state);\n\n        return (\n            <div>\n                <div><Link to='/'>Home</Link></div>\n                <h3>Blocks</h3>\n                <div>\n                    {\n                        [...Array(Math.ceil(this.state.blocksLength / 5)).keys()].map(key => {\n                            const paginatedId = key + 1;\n\n                            return (\n                                <span key={key} onClick={this.fetchPaginatedBlocks(paginatedId)}>\n                                    <Button bsSize=\"small\" bsStyle=\"danger\">\n                                        {paginatedId}\n                                    </Button>{' '}\n                                </span>\n                            );\n                        })\n                    }\n                </div>\n                {\n                    this.state.blocks.map((block) => {\n                        return (\n                            <Block key={block.hash} block={block} />\n                        )\n                    })\n                }\n            </div>\n        );\n    }\n\n}\nexport default Blocks;"},"lineCount":null}},"error":null,"hash":"5b053860edf06367067a5990b9064b2e","cacheData":{"env":{}}}